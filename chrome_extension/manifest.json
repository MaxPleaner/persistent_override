{
  "manifest_version": 2,

  "name": "persistent override",
  "description": "a self-hosted userscript system using Firebase",
  "version": "1.0",
  
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
